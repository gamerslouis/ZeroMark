{
    "env": {
        "browser": true,
        "es6": true,
        "webextensions": true,
        "jquery":true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "indent": [
            "off"
        ],
        "linebreak-style": [
            "error",
            "windows"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ],
        "no-console":[
            "off"
        ],
        "no-undef":"warn",
        "no-unused-vars":"warn"
    }
}